#: backends/gconf-merge-tree.c:121
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Gebruik: %s <map>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Gebruik: %s <map>\n"
"  Samenvoegen van opmaak bestandssysteem hiearchie, bijvoorbeeld:\n"
"    map/%%gconf.xml\n"
"      submap1/%%gconf.xml\n"
"      submap2/%%gconf.xml\n"
"  naar:\n"
"    map/%%gconf-tree.xml\n"

