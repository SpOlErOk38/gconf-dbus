#: backends/gconf-merge-tree.c:120
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Bruk: %s <katalog>\n"

#: backends/gconf-merge-tree.c:126
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Bruk: %s <katalog>\n"
"  Fletter et filsystemhierarki for motor som dette:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  til:\n"
"    dir/%%gconf-tree.xml\n"

