#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Utilização: %s <dir>\n"

#: backends/gconf-merge-tree.c:126
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Utilização: %s <dir>\n"
"  Mescla uma hierarquia de sistema de arquivos de suporte em etiquetas do "
"tipo:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  para:\n"
"    dir/%%gconf-tree.xml\n"

