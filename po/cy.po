msgstr "Gwall wrth arbed coeden GConf at '%s': %s\n"

#: backends/gconf-merge-tree.c:120
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Defnydd: %s <cyfeiriadur>\n"

#: backends/gconf-merge-tree.c:126
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Defnydd: %s <cyfeiriadur>\n"
"  Cyfuno system ffeiliau ochr gefn farciwyd i fyny, e.e:\n"
"    cyf/%%gconf.xml\n"
"        isgyf1/%%gconf.xml\n"
"        isgyf2/%%gconf.xml\n"
"  i mewn i:\n"
"    cyf/%%gconf-tree.xml\n"

