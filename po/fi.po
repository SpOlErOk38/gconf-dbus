#, c-format
msgid "Error saving GConf tree to '%s': %s\n"
msgstr "GConf-puun tallennus tiedostoon \"%s\" epäonnistui: %s\n"

#: backends/gconf-merge-tree.c:121
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Käyttö: %s <hakemisto>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Käyttö: %s <hakemisto>\n"
"  Yhdistää merkintämoduulin tiedostojärjestelmärakenteen seuraavasti:\n"
"    hak/%%gconf.xml\n"
"        alihak1/%%gconf.xml\n"
"        alihak2/%%gconf.xml\n"
"  tuloksena:\n"
"    hak/%%gconf-tree.xml\n"

