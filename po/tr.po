#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Kullanım: %s <dir>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Kullanım: %s <dir>\n"
"  Dosyasistemi hiyerarşisinin arkayüzündeki birleştirme farkları: \n"
"    dizin/%%gconf.xml\n"
"        altdizin1/%%gconf.xml\n"
"        altdizin1/%%gconf.xml\n"
"  şuraya:\n"
"    dizin/%%gconf-tree.xml\n"

