msgstr "Errore nel salvataggio dell'albero GConf su '%s': %s\n"

#: backends/gconf-merge-tree.c:121
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Uso: %s <dir>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Uso: %s <dir>\n"
"  Unisce una gerarchia di filesystem di markup come:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  in:\n"
"    dir/%%gconf-tree.xml\n"

